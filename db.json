            {
                "id": 123,
                "title": "Roasted Vegetable & Black Bean Tacos",
                "timeToPrepare": "60 minutes",
                "portions": 6,
                "imageUrl": "https://imagesvc.meredithcorp.io/v3/mm/image?url=https%3A%2F%2Fstatic.onecms.io%2Fwp-content%2Fuploads%2Fsites%2F44%2F2019%2F08%2F26230700%2F4499626.jpg&q=60g",
                "categories": [
                    3
                ],
                "ingredients": [
                    "1 cup roasted root vegetables (see associated recipe)",
                    "½ cup cooked or canned black beans, rinsed",
                    "2 teaspoons extra-virgin olive oil",
                    "1 teaspoon chili powder",
                    "½ teaspoon ground coriander",
                    "¼ teaspoon ground pepper",
                    "1 lime, cut into wedges",
                    "Chopped fresh cilantro & salsa for garnish"
                ],
                "preparationMethod": [
                    {
                        "step": 1,
                        "text": "Combine roasted root vegetables, beans, oil, cumin, chili powder, coriander, salt and pepper in a saucepan. Cover and cook over medium-low heat until heated through, 6 to 8 minutes."
                    },
                    {
                        "step": 2,
                        "text": "Divide the mixture among the tortillas. Top with avocado. Serve with lime wedges. Garnish with cilantro and/or salsa, if desired."
                    }
                ]
            },
            
            {
                "id": 156,
                "title": "Garlic-Anchovy Pasta with Broccolini",
                "timeToPrepare": "45 minutes",
                "portions": 6,
                "imageUrl": "https://ichef.bbci.co.uk/food/ic/food_16x9_832/recipes/penne_with_anchovy_19322_16x9.jpg",
                "categories": [
                    3
                ],
                "ingredients": [
                    "2 tablespoons extra-virgin olive oil",
                    "6 anchovy fillets (see Tip)",
                    "4 cloves garlic, thinly sliced",
                    "Pinch of crushed red pepper",
                    "1 (5 ounce) package baby spinach",
                    "8 ounces whole-wheat angel hair pasta",
                    "2 bunches broccolini or 1 bunch broccoli rabe, trimmed and coarsely chopped",
                    "¼ cup chopped fresh parsley"
                ],
                "preparationMethod": [
                    {
                        "step": 1,
                        "text": "Put a large pot of water on to boil."
                    },
                    {
                        "step": 2,
                        "text": "Meanwhile, heat oil in a large skillet over medium heat. Add anchovies, garlic and crushed red pepper; cook, pressing the anchovies with the back of a wooden spoon to break them up, until fragrant, about 2 minutes. Add spinach in 2 batches and cook, stirring occasionally, until just wilted, about 1 minute. Remove from heat and cover to keep warm."
                    },
                    {
                        "step": 3,
                        "text": "Add pasta and broccolini (or broccoli rabe) to the boiling water and cook until just tender, 3 to 5 minutes. Reserve 1 cup of the cooking water. Drain and transfer the pasta and vegetables to the skillet; toss to combine, adding enough of the reserved water to achieve desired consistency. Toss with parsley and salt and serve topped with almonds and goat cheese. Garnish with lemon zest, if desired."
                    }
                ]
            },
            
            
            {
                "id": 1536,
                "title": "Quinoa, Avocado & Chickpea Salad over Mixed Greens",
                "timeToPrepare": "45 minutes",
                "portions": 6,
                "imageUrl": "https://www.alphafoodie.com/wp-content/uploads/2022/03/Quinoa-Salad-A-bowl-with-quinoa-salad-with-veggies-and-a-wooden-spoon.jpeg",
                "categories": [
                    3
                ],
                "ingredients": [
                    "¼ teaspoon kosher salt or other coarse salt",
                    "⅓ cup quinoa",
                    "4 cloves garlic, thinly sliced",
                    "3 tablespoons lemon juice",
                    "1 (5 ounce) package baby spinach",
                    "8 ounces whole-wheat angel hair beans",
                    "¼ teaspoon ground pepper"
                ],
                "preparationMethod": [
                    {
                        "step": 1,
                        "text": "Bring water to a boil in a small saucepan. Stir in quinoa. Reduce heat to low, cover, and simmer until all the liquid is absorbed, about 15 minutes. Use a fork to fluff and separate the grains; let cool for 5 minutes.          "
                    },
                    {
                        "step": 2,
                        "text": "Meanwhile, sprinkle salt over garlic on a cutting board. Mash the garlic with the side of a spoon until a paste forms. Scrape into a medium bowl. Whisk in lemon zest, lemon juice, oil, and pepper. Transfer 3 Tbsp. of the dressing to a small bowl and set aside."
                    },
                    {
                        "step": 3,
                        "text": "Add chickpeas, carrot, and avocado to the bowl with the remaining dressing; gently toss to combine. Let stand for 5 minutes to allow flavors to blend. Add the quinoa and gently toss to coat."
                    }
                ]
            }
